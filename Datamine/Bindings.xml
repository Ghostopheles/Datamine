<Bindings>
    <Binding name="Toggle Datamine Model Viewer" header="Datamine" category="Datamine">
        if DatamineModelViewFrame:IsShown() then
        DatamineModelViewFrame:Hide();
        else
        Datamine.ModelView:Show();
        end
    </Binding>
    <Binding name="Toggle Datamine Explorer" category="Datamine">
        Datamine.Explorer:Toggle();
    </Binding>
    <Binding name="Toggle Datamine UI" category="Datamine">
        DatamineUnifiedFrame:Toggle();
    </Binding>
    <Binding name="Quick Reload" category="Datamine">
        Datamine.QuickReload();
    </Binding>
</Bindings>