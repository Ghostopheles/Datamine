<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Meorawr/wow-ui-schema/main/UI.xsd">
    <Texture name="DatamineCustomAtlasTemplate" mixin="DatamineCustomAtlasMixin"
        virtual="true">
        <!-- Add a .FileName keyValue to set the fileName used for the atlas lookup -->
        <!-- Add a .FilePath keyValue to set the filePath used for the atlas -->
        <!-- Add a .AtlasName keyValue to set the atlas name -->
        <!-- Add a .UseAtlasSize keyValue to set texture size to the size specified by the atlas -->
        <Scripts>
            <OnLoad method="OnLoad_Base" />
        </Scripts>
    </Texture>
</Ui>